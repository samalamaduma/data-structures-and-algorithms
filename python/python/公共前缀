str1 = input()
str2 = input()
str3 = input()
str4 = input()
lst1 = list(str1)
lst2 = list(str2)
lst3 = list(str3)
lst4 = list(str4)
lst = []
l = min([len(lst1), len(lst2), len(lst3), len(lst4)])
for i in range(l):
    if lst1[i] == lst2[i] == lst3[i] == lst4[i]:
        lst.append(lst1[i])
    else:
        break
print("".join(lst))